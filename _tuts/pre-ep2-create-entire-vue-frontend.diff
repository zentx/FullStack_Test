+/**
+ * Controller to render a basic "homepage".
+ */
+class FrontendController extends AbstractController
+{
+    /**
+     * @Route("/")
+     */
+    public function homepage()
+    {
+        return $this->render('frontend/homepage.html.twig');
+    }
